		</afsequence>
		<stemCells>
			<parameter id="alignment.stemCells" value="5"/>
		</stemCells>
	</afalignment>

	<AFsequenceErrorModel id="errorModel">
		<stemCells>
			<parameter idref="alignment.stemCells"/>
		</stemCells>
		<deltaOffset>
			<parameter id="errorModel.deltaOffset" value="0.2" lower="0.0" upper="1.0"/>
		</deltaOffset>
		<etaOffset>
			<parameter id="errorModel.etaOffset" value="0.7" lower="0.0" upper="1.0"/>
		</etaOffset>
		<kappaScale>
			<parameter id="errorModel.kappaScale" value="50" lower="0.0"/>
		</kappaScale>
	</AFsequenceErrorModel>

	<!-- DM TODO: Not using selection bias correction for now -->
	<!--
	<ascertainedCharacterPatterns id="patterns">
		<alignment idref="alignment"/>
		<state code='H'/>
	</ascertainedCharacterPatterns>
	-->

	<!-- DM TODO: For now, we do not have a way to deal with ambiguous or unknown characters, so we should probably strip them. I also think we do not have a way of specifying them either, so at this point this is not very relevant -->
	<!--
	<patterns id="patterns" strip="true">
		<alignment idref="alignment"/>
	</patterns>
	-->
	
	<!-- A prior assumption that the population size has remained constant       -->
	<!-- throughout the time spanned by the genealogy.                           -->
	<constantSize id="constant" units="years">
		<populationSize>
			<parameter id="constant.popSize" value="1" lower="0.0"/>
		</populationSize>
	</constantSize>
	
	<!-- Generate a random starting tree under the coalescent process            -->
	<coalescentSimulator id="startingTree">
		<taxa idref="taxa"/>
		<constantSize idref="constant"/>
	</coalescentSimulator>
	
	<!-- Generate a tree model                                  -->
	<treeModel id="treeModel">
		<coalescentTree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>
	
	<!-- Generate a coalescent likelihood                                        -->
	<!--
	<coalescentLikelihood id="coalescent">
		<model>
			<constantSize idref="constant"/>
		</model>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
	</coalescentLikelihood>
	-->
	<!-- The strict clock (Uniform rates across branches)                        -->
	
	<strictClockCenancestorBranchRates id="branchRates">
		<rate>
			<parameter id="clock.rate" value="0.05"/>
		</rate>
	</strictClockCenancestorBranchRates>
	
	<!-- The substitution model -->

	<flipflopModel id = "flipflopSubstitutionModel" normalize="false">
		<alignment idref="alignment"/>
		<stemCells>
			<parameter idref="alignment.stemCells"/>
        </stemCells>
        <mu>
            <parameter id="flipflop.mu" value="1" lower="0.0"/>
        </mu>
        <gamma>
            <parameter id="flipflop.relativeGamma" value="2" lower="0.0"/>
        </gamma>
        <lambda>
            <parameter id="flipflop.relativeLambda" value="10" lower="0.0"/>
		</lambda>
	</flipflopModel>
	
	<siteModel id="siteModel">
		<substitutionModel>
			<flipflopModel idref="flipflopSubstitutionModel"/>
		</substitutionModel>
	</siteModel>

	<!-- Programmatic initialization of regular frequencies -->
	<flipflopCenancestorFrequency id="cenancestorFrequencyModel" methylatedProportion="0.5">
		<alignment idref="alignment" />
		<frequencies>
			<parameter id="cenancestor.frequencies" value="1"/>
		</frequencies>
	</flipflopCenancestorFrequency>
	
	<!-- Manual alternative -->
	<!--
	<flipflopCenancestorFrequency id="cenancestorFrequencyModel">
		<alignment idref="alignment"/>
		<frequencies>
			<parameter id="cenancestor.frequencies" value="0.5 0 0 0 0 0.5"/>
		</frequencies>
	</flipflopCenancestorFrequency>
	-->
	<!-- DM TODO: this is not compatible with Ambiguities for now -->
	<cenancestorTreeLikelihood id="treeLikelihood" useAmbiguities="false" heightRules="true">
		<patterns idref="alignment"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel"/>
		<cenancestorFrequency idref="cenancestorFrequencyModel"/>
		<tipStatesModel idref="errorModel"/>
		<cenancestorHeight>
			<parameter id="luca_height" value="45"/>
		</cenancestorHeight>
		<cenancestorBranch>
			<parameter id="luca_branch" value="1" upper="45.0" lower="0.0"/>
			<!-- Value 1 as a safe starting value -->
		</cenancestorBranch>
		<strictClockCenancestorBranchRates idref="branchRates"/>
	</cenancestorTreeLikelihood>
	
	<operators id="operators" optimizationSchedule="default">

		<!-- Error -->
		<scaleOperator scaleFactor="0.9" weight="0.75">
	                <parameter idref="errorModel.deltaOffset"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.9" weight="0.75">
	                <parameter idref="errorModel.etaOffset"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.9" weight="0.75">
	                <parameter idref="errorModel.kappaScale"/>
		</scaleOperator>
		
		<!-- Substitution -->
		<scaleOperator scaleFactor="0.9" weight="0.75">
	                <parameter idref="flipflop.relativeGamma"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.9" weight="0.75">
	                <parameter idref="flipflop.relativeLambda"/>
		</scaleOperator>
		
		<!-- Clock -->
		<scaleOperator scaleFactor="0.9" weight="2.5">
			<parameter idref="clock.rate"/>
		</scaleOperator>
		
		<!-- Demography -->
		<!--
		<scaleOperator scaleFactor="0.5" weight="3.0">
			<parameter idref="constant.popSize"/>
		</scaleOperator>
		-->
		<!-- Tree -->
		<subtreeSlide size="2.5" gaussian="true" weight="15.0"> <!-- 2.5 years. They will be automatically optimized by BEAST though -->
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="15.0">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="3.0">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="3.0">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
		<scaleOperator scaleFactor="0.25" weight="3.0">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="30.0">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
        <upDownOperator scaleFactor="0.25" weight="3.0">
                <up>
                        <parameter idref="clock.rate"/>
                </up>
                <down>
                        <parameter idref="treeModel.allInternalNodeHeights"/>
                </down>
		</upDownOperator>
	</operators>
	
	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="750000" autoOptimize="true" operatorAnalysis="sim1ClockNoCoal.ops">
		<posterior id="posterior">
			<prior id="prior">
				<!-- Error -->
				<betaPrior shape="95.0" shapeB="5.0">
					<parameter idref="errorModel.etaOffset"/>
				</betaPrior>
				<betaPrior shape="5.0" shapeB="95.0">
					<parameter idref="errorModel.deltaOffset"/>
				</betaPrior>
				<logNormalPrior mean="4.56" stdev="0.3">
				<!-- Rounded from calculating the logscale mean and stdev for mean=100 and stdev=30-->
					<parameter idref="errorModel.kappaScale"/>
				</logNormalPrior>

				<!-- Substitution -->
				<logNormalPrior mean="0" stdev="1">
                    <parameter idref="flipflop.relativeGamma"/>
                </logNormalPrior>
                <logNormalPrior mean="3" stdev="1.5">
                    <parameter idref="flipflop.relativeLambda"/>
                </logNormalPrior>

				<!-- Clock -->
				<halfNormalPrior mean="0.0" stdev="0.05">
					<parameter idref="clock.rate"/>
				</halfNormalPrior>
				
				<!-- Demography -->
				<oneOnXPrior>
					<parameter idref="constant.popSize"/>
				</oneOnXPrior>
				
				<!-- Tree -->
					<!--<coalescentLikelihood idref="coalescent"/>-->
				<!-- Cenancestor Prior on the branch -->
	            <uniformPrior lower="0.0" upper="50.0">
					<parameter idref="luca_branch"/>
				</uniformPrior>
			</prior>
			<likelihood id="likelihood">
				<cenancestorTreeLikelihood idref="treeLikelihood"/>
			</likelihood>
		</posterior>
		<operators idref="operators"/>
	
		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="75">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
		</log>
	
		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="75" fileName="sim1ClockNoCoal.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="errorModel.deltaOffset"/>
			<parameter idref="errorModel.etaOffset"/>
			<parameter idref="errorModel.kappaScale"/>
			<parameter idref="flipflop.mu"/>
			<parameter idref="flipflop.relativeGamma"/>
			<parameter idref="flipflop.relativeLambda"/>
			<parameter idref="alignment.stemCells"/>
			<parameter idref="treeModel.rootHeight"/>
			<parameter idref="luca_height"/>
			<parameter idref="luca_branch"/>
			<parameter idref="constant.popSize"/>
			<parameter idref="clock.rate"/>
			<!--<coalescentLikelihood idref="coalescent"/>-->
		</log>
	
		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="75" nexusFormat="true" fileName="sim1ClockNoCoal.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
		</logTree>
	</mcmc>
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
</beast>
