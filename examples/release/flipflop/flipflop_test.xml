<?xml version="1.0" standalone="yes"?>
<beast>

    <!-- The list of taxa to be analysed (can also include dates/ages).          -->
    <taxa id="taxa">
        <taxon id="S1">
            <date value="60.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="S2">
            <date value="60.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="S3">
            <date value="60.0" direction="forwards" units="years"/>
        </taxon>
        <taxon id="S3_2">
            <date value="60.0" direction="forwards" units="years"/>
        </taxon>
    </taxa>

    <afalignment id="alignment">
        <afsequence>
            <taxon idref="S1"/>
            0.1,0.1,0.2,0.7,0.1,0.1,0.5,0.7,0.1,0.1,0.7
        </afsequence>
        <afsequence>
            <taxon idref="S2"/>
            0.14,0.5,0.4,1,0.1,0.1,0.5,0.7,0.1,0.1,0.75
        </afsequence>
        <afsequence>
            <taxon idref="S3"/>
            0.1,0.1,0.2,0.7,0.1,0.1,0.6,0.7,0.1,0.1,0.7
        </afsequence>
        <afsequence>
            <taxon idref="S3_2"/>
            0,0.1,0.1,0.7,0.1,0.1,0.6,0.17,0.21,0.1,0.7
        </afsequence>
        <states>
            <parameter id="alignment.states" value="5"/>
        </states>
    </afalignment>

    <AFsequenceErrorModel id="errorModel" type="transitions">
        <stemCells>
            <parameter id="errorModel.stemCells" value="2"/>
        </stemCells>
        <deltaOffset>
            <parameter id="errorModel.deltaOffset" value="5" lower="0.0" upper="100.0"/>
        </deltaOffset>
        <etaOffset>
            <parameter id="errorModel.etaOffset" value="95" lower="0.0" upper="100.0"/>
        </etaOffset>
        <kappaScale>
            <parameter id="errorModel.kappaScale" value="0.1"/>
        </kappaScale>
    </AFsequenceErrorModel>

    <flipflopModel id = "model">
        <stemCells>
            <parameter id="model.cells" value="2"/>
        </stemCells>
        <mu>
            <parameter id="model.mu" value="0.1"/>
        </mu>
        <gamma>
            <parameter id="model.gamma" value="0.1"/>
        </gamma>
        <lambda>
            <parameter id="model.lambda" value="1"/>
        </lambda>
    </flipflopModel>

</beast>